

drop table movtest;
drop table pairingtest;
drop table tagtest;

CREATE TABLE movtest (
    id integer NOT NULL,
    title character varying(255),
    year integer,
    rating character varying(10),
    duration integer,
    poster character varying(255),
    PRIMARY KEY (id)
);



CREATE TABLE pairingtest (
    id integer NOT NULL,
    tag_id integer,
    movie_id integer, 
    PRIMARY KEY (id)
);



CREATE TABLE tagtest (
    id integer NOT NULL,
    name character varying(255),
    type character varying(255),
    PRIMARY KEY (id)
);



